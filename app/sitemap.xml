import type { MetadataRoute } from "next";

export default function sitemap(): MetadataRoute.Sitemap {
  const baseUrl = "https://humlumdartklub.com";

  return [
    { url: `${baseUrl}/`, changeFrequency: "weekly", priority: 1.0 },

    // ret stierne s√• de matcher jeres rigtige routes:
    { url: `${baseUrl}/om-klubben`, changeFrequency: "monthly", priority: 0.8 },
    { url: `${baseUrl}/events`, changeFrequency: "weekly", priority: 0.7 },
    { url: `${baseUrl}/bliv-medlem`, changeFrequency: "monthly", priority: 0.9 },
    { url: `${baseUrl}/sponsor`, changeFrequency: "monthly", priority: 0.7 },
    { url: `${baseUrl}/konkurrencer`, changeFrequency: "weekly", priority: 0.7 },
  ];
}
